    "presente": {
      "spanish": "Ella habla español.",
      "english": "She speaks Spanish."
    },
    "preterito": {
      "spanish": "Él habló con su amigo ayer.",
      "english": "He spoke with his friend yesterday."
    },
    "imperfecto": {
      "spanish": "Yo hablaba mucho cuando era niño.",
      "english": "I used to speak a lot when I was a child."
    },
    "futuro": {
      "spanish": "Nosotros hablaremos mañana.",
      "english": "We will speak tomorrow."
    },
    "condicional": {
      "spanish": "Ustedes hablarían si tuvieran tiempo.",
      "english": "You all would speak if you had time."
    },
    "perfecto": {
      "spanish": "Yo he hablado con mi jefe.",
      "english": "I have spoken with my boss."
    }
