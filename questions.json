  "tenseExplanations": {
    "presente": "Used for actions happening now, habitual actions, and general truths.",
    "preterito": "Used for completed actions at a specific time in the past.",
    "imperfecto": "Used for ongoing, habitual, or descriptive actions in the past.",
    "futuro": "Used for actions that will happen in the future.",
    "condicional": "Used for hypothetical actions or things that 'would' happen.",
    "perfecto": "Used for actions that have happened recently or have a connection to the present."
  },
